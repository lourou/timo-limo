{
  "name": "timo-limo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:cf": "npm run build:cf && npx wrangler pages dev .vercel/output/static --compatibility-date=2024-09-25 --d1=DB=timo-limo-2025-09 --r2=R2=timo-limo-2025-09",
    "build": "next build",
    "build:cf": "npx @cloudflare/next-on-pages",
    "preview": "npm run dev:cf",
    "deploy": "npm run build:cf && npx wrangler pages deploy .vercel/output/static --project-name timo-limo",
    "cf-typegen": "npx wrangler types",
    "db:create": "npx wrangler d1 create timo-limo-db",
    "db:schema": "npx wrangler d1 execute timo-limo-db --file=./schema.sql",
    "db:schema:remote": "npx wrangler d1 execute timo-limo-db --file=./schema.sql --remote"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "js-cookie": "^3.0.0",
    "next": "15.0.3",
    "qrcode": "^1.5.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "^0.33.0",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1",
    "@types/js-cookie": "^3.0.0",
    "@types/node": "^22.0.0",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.0.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
